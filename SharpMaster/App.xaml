<Application x:Class="SharpMaster.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SharpMaster.Infrastucture"
             StartupUri="Views/Windows/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/AppStyles.xaml"/>
                <ResourceDictionary Source="Styles/UIElements/ButtonsStyle.xaml"/>
                <ResourceDictionary Source="Styles/UIElements/BordersStyle.xaml"/>
                <ResourceDictionary Source="Styles/UIElements/TextBlockStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <local:DI x:Key="DI"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
